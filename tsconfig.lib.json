{
  "compilerOptions": {
    "module": "esnext",
    "target": "ES2019",
    "lib": [
      "dom",
      "esnext"
    ],
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "downlevelIteration": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "src/*",
        "node_modules/*"
      ]
    },
    "jsx": "react",
    "esModuleInterop": true,
  }
}
